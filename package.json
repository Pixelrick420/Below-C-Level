{
  "name": "below-c-level",
  "displayName": "below_c_level",
  "description": "The most useless VS Code extension ever created",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "below-c-level.getJoke",
        "title": "Below C Level: Get Joke"
      },
      {
        "command": "below-c-level.nameChanger",
        "title": "Below C Level: Shake it"
      },
      {
        "command": "below-c-level.snake",
        "title": "Below C Level: Snake"
      },
      {
        "command": "below-c-level.snake.up",
        "title": "Snake Up"
      },
      {
        "command": "below-c-level.snake.down",
        "title": "Snake Down"
      },
      {
        "command": "below-c-level.snake.left",
        "title": "Snake Left"
      },
      {
        "command": "below-c-level.snake.right",
        "title": "Snake Right"
      },
      {
        "command": "below-c-level.openDashboard",
        "title": "Below C Level: Open Dashboard"
      },
      {
        "command": "below-c-level.openSettings",
        "title": "Below C Level: Open Settings"
      },
      {
        "command": "below-c-level.generateComments",
        "title": "Get comments"
      },
      {
        "command": "below-c-level.fibonacci",
        "title": "Below C Level: Fibonacci"
      }
    ],
    "keybindings": [
      {
        "key": "up",
        "command": "below-c-level.snake.up",
        "when": "below-c-level.snakeActive"
      },
      {
        "key": "down", 
        "command": "below-c-level.snake.down",
        "when": "below-c-level.snakeActive"
      },
      {
        "key": "left",
        "command": "below-c-level.snake.left", 
        "when": "below-c-level.snakeActive"
      },
      {
        "key": "right",
        "command": "below-c-level.snake.right",
        "when": "below-c-level.snakeActive"
      }
    ],
    "configuration": {
      "title": "Below C Level",
      "properties": {
        "belowCLevel.autoNameChange": {
          "type": "boolean",
          "default": false,
          "description": "Automatically replace identifiers with Shakespearean insults"
        },
        "belowCLevel.autoSnake": {
          "type": "boolean",
          "default": false,
          "description": "Randomly spawn code-eating snake"
        },
        "belowCLevel.autoJoke": {
          "type": "boolean",
          "default": false,
          "description": "Randomly tell stupid tech jokes"
        },
        "belowCLevel.snakeSpawnChance": {
          "type": "number",
          "default": 0.1,
          "minimum": 0,
          "maximum": 1,
          "description": "Chance of snake spawning (0-1)"
        },
        "belowCLevel.jokeFrequency": {
          "type": "number",
          "default": 300000,
          "description": "Joke frequency in milliseconds"
        },
        "belowCLevel.nameChangeDelay": {
          "type": "number",
          "default": 5000,
          "description": "Delay before auto name change in milliseconds"
        },
        "belowCLevel.autoFibonacci": {
          "type": "boolean",
          "default": false,
          "description": "Automatically apply Fibonacci indentation to Python files"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}