{
  "name": "below-c-level",
  "displayName": "below_c_level",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "below-c-level.getJoke",
        "title": "Below C Level: Get Joke"
      },
      {
        "command": "below-c-level.nameChanger",
        "title": "Below C Level: Shake it"
      },
      {
        "command": "below-c-level.snake",
        "title": "Below C Level: Snake"
      },
      {
        "command": "below-c-level.snake.up",
        "title": "Snake Up"
      },
      {
        "command": "below-c-level.snake.down",
        "title": "Snake Down"
      },
      {
        "command": "below-c-level.snake.left",
        "title": "Snake Left"
      },
      {
        "command": "below-c-level.snake.right",
        "title": "Snake Right"
      },
      {
        "command": "below-c-level.generateComment",
        "title": "Add comment"
      }
    ],
    "keybindings": [
      {
        "key": "up",
        "command": "below-c-level.snake.up",
        "when": "editorTextFocus"
      },
      {
        "key": "down",
        "command": "below-c-level.snake.down",
        "when": "editorTextFocus"
      },
      {
        "key": "left",
        "command": "below-c-level.snake.left",
        "when": "editorTextFocus"
      },
      {
        "key": "right",
        "command": "below-c-level.snake.right",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}